											<a href="/user/mypage" class="flex-c-m size1 bg1 bo-rad-20 hov1 s-text1 trans-0-4">
												My page
											</a>
										</div>
									<% } %>
								</div>
							</div>
						</div>

					<span class="linedivide1"></span>

					<div class="header-wrapicon2">
						<img src="./images/icons/icon-header-02.png" class="header-icon1 js-show-header-dropdown" alt="ICON">
						<span class="header-icons-noti"><%= basketCount %></span>
						<% if(typeof user_id == 'undefined'){ %>
							<div class="header-cart header-dropdown">
									<ul class="header-cart-wrapitem">
											<li class="header-cart-item">
												Login please :)
											</li>
									</ul>
							</div>
						<% }else{ %>
						<!-- Header cart noti -->
						<div class="header-cart header-dropdown">
								<% for(let i in userBasket){ %>
									<ul class="header-cart-wrapitem">
										<li class="header-cart-item">
											<a href="/product-detail?name=<%=userBasket[i].productNameKey%>&price=<%=userBasket[i].products.productPrice%>&highCategoryFilter=<%=userBasket[i].highCategoryFilter%>&lowCategoryFilter=<%=userBasket[i].lowCategoryFilter%>&description=<%=userBasket[i].description%>">
												<div class="header-cart-item-img">
													<img src="./images/products/<%=userBasket[i].highCategoryFilter%>/<%=userBasket[i].lowCategoryFilter%>/720960/<%=userBasket[i].products.productColor%>.jpg" onerror="this.onerror=null;this.src='./images/ready.jpg'" alt="IMG">
												</div>
											</a>
	
											<div class="header-cart-item-txt">
												<a href="/product-detail?name=<%=userBasket[i].productNameKey%>&price=<%=userBasket[i].products.productPrice%>&highCategoryFilter=<%=userBasket[i].highCategoryFilter%>&lowCategoryFilter=<%=userBasket[i].lowCategoryFilter%>&description=<%=userBasket[i].description%>" class="header-cart-item-name">
													<%=userBasket[i].products.productName%>
												</a>
	
												<span class="header-cart-item-info">
													<%=userBasket[i].productCountKey%> x <%=userBasket[i].products.productPrice%> ₩
												</span>
											</div>
										</li>
									</ul>	
								<% } %>
								<div class="header-cart-total">
									Total: <%=totalPrice%>
								</div>
	
								<div class="header-cart-buttons">
									<div class="header-cart-wrapbtn">
										<!-- Button -->
										<a href="/cart" class="flex-c-m size1 bg1 bo-rad-20 hov1 s-text1 trans-0-4">
											View Cart
										</a>
									</div>
	
									<div class="header-cart-wrapbtn">
										<!-- Button -->
										<a href="/service/payment" class="flex-c-m size1 bg1 bo-rad-20 hov1 s-text1 trans-0-4">
											Check Out
										</a>
									</div>
								</div>
						</div>
					<% } %>
					</div>
				</div>
			</div>
		</div>

		<!-- Header Mobile -->
		<div class="wrap_header_mobile">
			<!-- Logo moblie -->
			<a href="/" class="logo-mobile">
				<img src="./images/icons/logo.png" alt="IMG-LOGO">
			</a>

			<!-- Button show menu -->
			<div class="btn-show-menu">
				<!-- Header Icon mobile -->
				<div class="header-icons-mobile">
					<a href="#" class="header-wrapicon1 dis-block">
						<img src="./images/icons/icon-header-01.png" class="header-icon1" alt="ICON">
