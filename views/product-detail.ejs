	<script>
		$('#search_2').click(() => {
			let data = prompt("검색할 제품을 입력해주세요");
			if(data){
				$.ajax({
					url : '/service/search',
					dataType : 'json',
					type : 'POST',
					data : {'productName' : data},
					success : result => {
						location.href=`${result.path}`;
					}
				});
			}else{
				alert("제대로 입력해주세요");
			}
		});
	</script>
	<script>
		$('#likeCount').click(() => {
			var n = $('#likeCount').index(this);
			var num = $("#likeCountText:eq("+n+")").val();
			num = $("#likeCountText:eq("+n+")").val(num*1+1); 
		});
	</script>
