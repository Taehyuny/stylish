										<a href="/join" class="flex-c-m size1 bg1 bo-rad-20 hov1 s-text1 trans-0-4">
											Sign up
										</a>
									</div>
									<% }else{ %>
										<div class="header-cart-wrapbtn">
											<!-- Button -->
											<a href="/logout" class="flex-c-m size1 bg1 bo-rad-20 hov1 s-text1 trans-0-4">
												Sign out
											</a>
										</div>
										<div class="header-cart-wrapbtn">
