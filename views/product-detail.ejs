	<!-- Product Detail -->
	<div class="container bgwhite p-t-35 p-b-80">
		<div class="flex-w flex-sb">
			<div class="w-size13 p-t-30 respon5">
				<div class="wrap-slick3 flex-sb flex-w">
					<div class="wrap-slick3-dots"></div>

					<div class="slick3">
						<div class="item-slick3" data-thumb="./images/products/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/720960/black.jpg">
							<div class="wrap-pic-w"> <!-- 1200 x 1600 -->
								<img src="./images/products/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/12001600/black.jpg" onerror="this.onerror=null;this.src='./images/ready.jpg'" alt="준비중인 상품입니다 ^^">
							</div>
						</div>

						<% if(highCategoryFilter == "bag"){ %>
							<div class="item-slick3" data-thumb="./images/products/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/720960/blue.jpg">
								<div class="wrap-pic-w">
									<img src="./images/products/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/12001600/blue.jpg" onerror="this.onerror=null;this.src='./images/ready.jpg'" alt="준비중인 상품입니다 ^^">
								</div>
							</div>
						<% }else{ %>
							<div class="item-slick3" data-thumb="./images/products/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/720960/gray.jpg" >
								<div class="wrap-pic-w">
									<img src="./images/products/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/12001600/gray.jpg" onerror="this.onerror=null;this.src='./images/ready.jpg'" alt="준비중인 상품입니다 ^^">
								</div>
							</div>
						<% } %>
						

						<div class="item-slick3" data-thumb="./images/products/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/720960/white.jpg">
							<div class="wrap-pic-w">
								<img src="./images/products/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/12001600/white.jpg" onerror="this.onerror=null;this.src='./images/ready.jpg'" alt="준비중인 상품입니다 ^^ ">
							</div>
						</div>
					</div>
				</div>
			</div>

			
			<div class="w-size14 p-t-30 respon5">
				<h2 class="product-detail-name" id="productName" name="productName"><%=productName%></h4>
<br><br>
				<span class="m-text17" id="productPrice" name="productPrice"><%=productPrice%></span>&nbsp;&nbsp;&nbsp;<span>₩</span>
<br><br>
				<!-- <input type="text" id="highCategoryFilter" name="highCategoryFilter" value="<%=highCategoryFilter%>" hidden>
				<input type="text" id="lowCategoryFilter" name="lowCategoryFilter" value="<%=lowCategoryFilter%>" hidden> -->
				<div class=" size9 trans-0-4 m-t-10 m-b-10">
					<!-- Button -->
					<button class="flex-c-m sizefull bg1  hov1 s-text1 trans-0-4" id="detail">
						Detail option
					</button>
				</div>
				<!--  -->
				<form action="/cart" method="POST">
					<input type="text" id="highCategoryFilter" name="highCategoryFilter" value="<%=highCategoryFilter%>" hidden>
					<input type="text" id="lowCategoryFilter" name="lowCategoryFilter" value="<%=lowCategoryFilter%>" hidden>
					<input type="text" id="productName" name="productNameKey" value="<%=productName%>" hidden>
					<input type="text" id="productName" name="productPriceKey" value="<%=productPrice%>" hidden>
					<input type="text" id="description" name="description" value="<%=description%>" hidden>
					<input type="text" id="user_id" name="user_id" value="<%=user_id%>" hidden>
					<!-- <input type="text" id="productCount" name="productCountKey" value="<%=description%>" hidden> -->
					<div class="p-t-33 p-b-60">
						<% if(highCategoryFilter == "shoes"){ %>
							<div class="flex-m flex-w p-b-10">
								<div class="s-text15 w-size15 t-center">
									Size
								</div>
			
								<div class="rs2-select2 rs3-select2 bo4 of-hidden w-size16">
									<select class="selection-2" name="productSizeKey" id="size" required>
										<option>Choose an option</option>
										<option>250</option>
										<option>260</option>
										<option>270</option>
										<option>280</option>
										<option>290</option>
										<option>300</option>
									</select>
								</div>
							</div>	
						<% }else if(highCategoryFilter == "shirt" || highCategoryFilter == "pants"){ %>
							<div class="flex-m flex-w p-b-10">
								<div class="s-text15 w-size15 t-center">
									High
								</div>
				
								<div class="rs2-select2 rs3-select2 bo4 of-hidden w-size16">
									<select class="selection-2" name="productHighKey" id="high" required>
										<option>Choose an option</option>
										<option>150-160</option>
										<option>160-170</option>
										<option>170-180</option>
										<option>180-190</option>
									</select>
								</div>
							</div>
							<div class="flex-m flex-w p-b-10">
								<div class="s-text15 w-size15 t-center">
									Weight
								</div>
					
								<div class="rs2-select2 rs3-select2 bo4 of-hidden w-size16">
									<select class="selection-2" name="productWeightKey" id="weight" required>
										<option>Choose an option</option>
										<option>50-70</option>
										<option>70-90</option>
										<option>90-110</option>
										<option>110-120</option>
									</select>
								</div>
							</div>		
						<% } %>
						<div class="flex-m flex-w">
							<div class="s-text15 w-size15 t-center">
								Color
							</div>

							<div class="rs2-select2 rs3-select2 bo4 of-hidden w-size16">
								<% if(lowCategoryFilter == "denim_shirt" || lowCategoryFilter == "blue_jeans"){ %>
									<select class="selection-2" name="productColorKey" id="color" required>
										<option>Choose an option</option>
										<option>Blue</option>
									</select>
								<% }else if(highCategoryFilter == "bag"){ %>
									<select class="selection-2" name="productColorKey" id="color" required>
										<option>Choose an option</option>
										<option>black</option>
										<option>blue</option>
										<option>white</option>
									</select>
								<% }else{ %>
									<select class="selection-2" name="productColorKey" id="color" required>
										<option>Choose an option</option>
										<option>gray</option>
										<option>black</option>
										<option>white</option>
									</select>
								<% } %>
							</div>
						</div>
					
				
<br>
					<div class="flex-r-m flex-w p-t-10">
						<div class="w-size16 flex-m flex-w">
							<div class="flex-w bo5 of-hidden m-r-22 m-t-10 m-b-10">
								<button class="btn-num-product-down color1 flex-c-m size7 bg8 eff2">
									<i class="fs-12 fa fa-minus" aria-hidden="true"></i>
								</button>

								<input class="size8 m-text18 t-center num-product" type="number" name="productCountKey" id="productCount" value="1">

								<button class="btn-num-product-up color1 flex-c-m size7 bg8 eff2">
									<i class="fs-12 fa fa-plus" aria-hidden="true"></i>
								</button>
							</div>

							<div class="btn-addcart-product-detail size9 trans-0-4 m-t-10 m-b-10">
								<!-- Button -->
								<!-- <button class="flex-c-m sizefull bg1 bo-rad-23 hov1 s-text1 trans-0-4" id="addCart">
									Add to Cart
								</button> -->
								<input type="submit" value="Add to Cart"  class="flex-c-m sizefull bg1 bo-rad-23 hov1 s-text1 trans-0-4">
							</div>
						</form>

							<div class=" size9 trans-0-4 m-t-10 m-b-10">
								<!-- Button -->
								<button class="flex-c-m sizefull bg1 bo-rad-23 hov1 s-text1 trans-0-4" id="viewModel" style="margin-left: 160px">
									View model
								</button>
							</div>
						</div>
					</div>
				</div>

				<!--  -->
				<div class="wrap-dropdown-content bo6 p-t-15 p-b-14 active-dropdown-content">
					<h5 class="js-toggle-dropdown-content flex-sb-m cs-pointer m-text19 color0-hov trans-0-4">
						Description
						<i class="down-mark fs-12 color1 fa fa-minus dis-none" aria-hidden="true"></i>
						<i class="up-mark fs-12 color1 fa fa-plus" aria-hidden="true"></i>
					</h5>

					<div class="dropdown-content dis-none p-t-15 p-b-23">
						<p class="s-text8">
							<%=description%>
						</p>
						<input type="text" id="description" name="description" value="<%=description%>" hidden>
					</div>
				</div>


			</div>
			</form>
		</div>
	</div>

	<!-- Footer -->
	<footer class="bg6 p-t-45 p-b-43 p-l-45 p-r-45">
		<div class="flex-w p-b-90">
			<div class="w-size6 p-t-30 p-l-15 p-r-15 respon3">
