					<div class="flex-m p-t-30">
						<a href="javascript:alert('facebook');" class="fs-18 color1 p-r-20 fa fa-facebook"></a>
						<a href="javascript:alert('instagram');" class="fs-18 color1 p-r-20 fa fa-instagram"></a>
						<a href="javascript:alert('pinterest');" class="fs-18 color1 p-r-20 fa fa-pinterest-p"></a>
						<a href="javascript:alert('snapchat');" class="fs-18 color1 p-r-20 fa fa-snapchat-ghost"></a>
						<a href="javascript:alert('youtube');" class="fs-18 color1 p-r-20 fa fa-youtube-play"></a>
					</div>
				</div>
			</div>

			<div class="w-size7 p-t-30 p-l-15 p-r-15 respon4">
