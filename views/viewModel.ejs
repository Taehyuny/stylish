<body>
    <% if(highCategoryFilter == "shirt" || highCategoryFilter == "pants"){ %>
        <img src="./images/models/simple/<%=highCategoryFilter%>/<%=lowCategoryFilter%>/high=<%=high%>&weight=<%=weight%>&color=<%=color%>.jpg" onerror="this.onerror=null;this.src='./images/ready.jpg'" alt="해당 모델은 준비중입니다." >
    <% }else if(highCategoryFilter == "shoes"){ %>
        <img src="./images/products/shoes/<%=lowCategoryFilter%>/720960/<%=color%>.jpg" onerror="this.onerror=null;this.src='./images/ready.jpg'" alt="해당 모델은 준비중입니다.">
    <% }else{ %>
        <img src="./images/products/bag/<%=lowCategoryFilter%>/720960/<%=color%>.jpg" onerror="this.onerror=null;this.src='./images/ready.jpg'" alt="해당 모델은 준비중입니다.">
    <% } %> 

    <div class="btn-addcart-product-detail size9 trans-0-4 m-t-10 m-b-10">
        <!-- Button -->
        <button class="flex-c-m sizefull bg1 bo-rad-23 hov1 s-text1 trans-0-4" id="back" onclick="history.back();">
            Back
        </button>
    </div>
</body>
